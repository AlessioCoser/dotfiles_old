{
    "workbench.colorCustomizations": {
      "[Material Theme]": {
        "sideBar.background": "#202c31",
        "editor.background": "#243238",
        "sideBar.border": "#243238",
        "terminal.border": "#243238",
        "panel.background": "#202c31"
      },
      "activityBarBadge.background": "#80CBC4",
      "activityBar.activeBorder": "#80CBC4",
      "list.activeSelectionForeground": "#80CBC4",
      "list.inactiveSelectionForeground": "#80CBC4",
      "list.highlightForeground": "#80CBC4",
      "scrollbarSlider.activeBackground": "#80CBC450",
      "editorSuggestWidget.highlightForeground": "#80CBC4",
      "textLink.foreground": "#80CBC4",
      "progressBar.background": "#80CBC4",
      "pickerGroup.foreground": "#80CBC4",
      "tab.activeBorder": "#80CBC4",
      "notificationLink.foreground": "#80CBC4",
      "editorWidget.resizeBorder": "#80CBC4",
      "editorWidget.border": "#80CBC4",
      "settings.modifiedItemIndicator": "#80CBC4",
      "settings.headerForeground": "#80CBC4",
      "panelTitle.activeBorder": "#80CBC4",
      "breadcrumb.activeSelectionForeground": "#80CBC4",
      "menu.selectionForeground": "#80CBC4",
      "menubar.selectionForeground": "#80CBC4",
      "editor.findMatchBorder": "#80CBC4",
      "selection.background": "#80CBC440"
    },
    "window.zoomLevel": 0,
    "editor.fontSize": 18,
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "explorer.openEditors.visible": 0,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Atom One Light",
    "workbench.iconTheme": "eq-material-theme-icons",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/.vagrant": true,
      "**/.serverless": true,
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.vagrant/**": true,
        "**/.serverless/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.vagrant": true,
        "**/.serverless": true
    },
    "newFile": {
      "defaultBaseFileName": "",
      "relativeTo": "project", // "root" or "project"
      "defaultFileExtension": ".js",
      "rootDirectory": "~",
      "showPathRelativeTo": "none", // "project" or "none"
      "expandBraces": false // setting to true will allow for creating multiple files such as `new-folder/{file1,file2}.js`
    },
    "standard.enable": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "extensions.ignoreRecommendations": true,
    "sublimeTextKeymap.promptV3Features": true,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "explorer.confirmDelete": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.activityBar.visible": true,
    "workbench.statusBar.feedback.visible": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "materialTheme.accent": "Teal",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "[html]": {
      "editor.defaultFormatter": "mohd-akram.vscode-html-format"
    },
    "markdown.marp.enableHtml": true,
    "markdown.marp.chromePath": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome",
    "js/ts.implicitProjectConfig.checkJs": true,
    "eslint.alwaysShowStatus": true
  }
