{
    "workbench.colorCustomizations": {
      "[Material Theme]": {
        "sideBar.background": "#202c31",
        "editor.background": "#243238",
        "sideBar.border": "#243238",
        "terminal.border": "#243238",
        "panel.background": "#202c31"
      }
    },
    "window.zoomLevel": 0,
    "editor.fontSize": 18,
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "explorer.openEditors.visible": 0,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Material Theme",
    "workbench.iconTheme": "eq-material-theme-icons",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.vagrant": true,
        "**/.serverless": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.vagrant/**": true,
        "**/.serverless/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.vagrant": true,
        "**/.serverless": true
    },
    "newFile": {
      "defaultBaseFileName": "",
      "relativeTo": "project", // "root" or "project"
      "defaultFileExtension": ".js",
      "rootDirectory": "~",
      "showPathRelativeTo": "none", // "project" or "none"
      "expandBraces": false // setting to true will allow for creating multiple files such as `new-folder/{file1,file2}.js`
    },
    "standard.enable": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "extensions.ignoreRecommendations": true,
    "sublimeTextKeymap.promptV3Features": true,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "explorer.confirmDelete": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.activityBar.visible": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.view.alwaysShowHeaderActions": true
  }
